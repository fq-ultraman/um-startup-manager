{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "UM启动项管理",
  "version": "0.1.3",
  "identifier": "com.fq-ultraman.UMStartupManager",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "UM Startup Manager",
        "width": 800,
        "height": 600,
        "decorations": false,
        "center": true,
        "resizable": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false
  }
}
